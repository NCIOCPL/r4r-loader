{
  "name": "r4rimporter",
  "version": "1.2.0",
  "description": "Content Importer for the Resources for Researchers API",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --collectCoverageFrom=lib/**.js --coverage lib"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "<rootDir>/lib/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/.*\\.json/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NCIOCPL/r4rimporter.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NCIOCPL/r4rimporter/issues"
  },
  "engines": {
    "node": ">=16.0.0 <17.0.0",
    "npm": ">=8.0.0 <9.0.0"
  },
  "homepage": "https://github.com/NCIOCPL/r4rimporter#readme",
  "dependencies": {
    "@elastic/elasticsearch": "^7.17.0",
    "@octokit/rest": "^19.0.4",
    "axios": "^0.27.2",
    "config": "^3.3.8",
    "elastic-tools": "git+https://github.com/nciocpl/elastic-tools.git#v2.0.0",
    "gray-matter": "^4.0.3",
    "loader-pipeline": "git+https://github.com/nciocpl/loader-pipeline.git#v1.1.0",
    "moment": "^2.29.4",
    "parse-github-repo-url": "^1.4.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/jest": "^29.0.1",
    "@types/node": "^16.11.58",
    "jest": "^29.0.3",
    "nock": "^13.2.9",
    "rehype-preset-minify": "^5.1.0",
    "rehype-stringify": "^8.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "remark-stringify": "^9.0.1",
    "unified": "^9.2.2",
    "winston-null-transport": "git+https://github.com/NCIOCPL/winston-null-transport.git#1.0.0"
  }
}
