{
  "name": "r4r-loader",
  "version": "3.0.0",
  "description": "Content Importer for the Resources for Researchers API",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --collectCoverageFrom=lib/**.js --coverage lib"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "<rootDir>/lib/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/.*\\.json/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NCIOCPL/r4r-loader.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NCIOCPL/r4r-loader/issues"
  },
  "engines": {
    "node": ">=20.11.0 <21.0.0"
  },
  "homepage": "https://github.com/NCIOCPL/r4r-loader#readme",
  "dependencies": {
    "@elastic/elasticsearch": "^7.17.13",
    "config": "^3.3.11",
    "elastic-tools": "github:nciocpl/elastic-tools#v3.0.0",
    "gray-matter": "^4.0.3",
    "loader-pipeline": "git+https://github.com/nciocpl/loader-pipeline.git#v1.1.0",
    "moment": "^2.30.1",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^16.18.89",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "rehype-preset-minify": "^5.1.0",
    "rehype-stringify": "^8.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "remark-stringify": "^9.0.1",
    "unified": "^9.2.2",
    "winston-null-transport": "git+https://github.com/NCIOCPL/winston-null-transport.git#1.0.0"
  }
}
